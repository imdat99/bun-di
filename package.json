{
  "name": "hono-di-monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/vite/client",
    "examples/*"
  ],
  "scripts": {
    "build": "bun run --filter @hono-di/core build && bun run --filter '*' --filter '!@hono-di/core' build",
    "test": "bun run --filter '*' test",
    "gen": "bun run packages/cli/src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.4.0"
  }
}