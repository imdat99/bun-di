{
    "name": "@hono-di/client",
    "version": "0.0.1",
    "description": "Type-safe client generator for Hono-DI",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "test": "bun test"
    },
    "dependencies": {
        "ts-morph": "^21.0.1",
        "glob": "^10.3.10"
    },
    "devDependencies": {
        "tsup": "^8.5.1",
        "@swc/core": "^1.15.8",
        "typescript": "^5.4.0",
        "@types/node": "^22.10.6"
    }
}